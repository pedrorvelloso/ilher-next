  "scripts": {
    "build": "yarn build:css && remix build",
    "prebuild": "yarn clear",
    "dev": "./scripts/run-dev",
    "postinstall": "remix setup node",
    "predeploy": "yarn clear",
    "start": "remix-serve build",
    "start:dev": "remix dev",
    "watch:css": "postcss styles --base styles --dir app/styles -w",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "check:types": "tsc -b",
    "check:lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "validate": "yarn check:lint",
    "prepare": "husky install",
    "clear": "rimraf .cache api/build public/build \"./app/styles/**/*.css\""
  },
  "dependencies": {
